{
  "name": "Quickwords",
  "version": "v0.0.4-alpha",
  "description": "An open-source alternative to Textexpander.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "node_modules/.bin/eslint --ext .js,.vue src",
    "pack": "build --dir",
    "dist": "build --icon=\"assets/icon.icns\""
  },
  "build": {
    "appId": "co.quickwords.quickwords",
    "productName": "Quickwords",
    "mac": {
      "directories": {
        "buildResources": "assets"
      }
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Dariusz Czajkowski and Grzegorz TÅ‚uszcz",
  "license": "MIT",
  "devDependencies": {
    "@vue/devtools": "^1.1.0",
    "cross-env": "^5.1.4",
    "electron": "^1.8.6",
    "electron-builder": "^20.13.3",
    "electron-reload": "^1.2.2",
    "eslint": "^4.19.1",
    "laravel-mix": "^2.1.11",
    "node-sass": "^4.9.0",
    "tailwindcss": "^0.5.3"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-is-dev": "^0.3.0",
    "iohook": "^0.1.16",
    "lodash": "^4.17.10",
    "native-keymap": "^1.2.5",
    "node-fetch": "^2.1.2",
    "robotjs": "^0.5.1",
    "vue": "^2.5.16",
    "webpack-merge": "^4.1.2"
  },
  "iohook": {
    "targets": [
      "node-51",
      "electron-57"
    ],
    "platforms": [
      "darwin"
    ],
    "arches": [
      "x64"
    ]
  }
}
